#include "rotational_matrix.h"
#include "params.h"
#include "user_define.h"
#include <math.h>
#include "bno055.h"

int get_rotational_matrix(float *roll, float *pitch,float *yaw, float *T_matrix[3][3]){
	uint8_t tmp;
	float Rx[3][3],Ry[3][3],Rz[3][3];
//	float roll_in,pitch_in,yaw_in;
//	roll_in  = *roll; pitch_in = *pitch; yaw_in = *yaw;
	Rx[0][0] = 1; Rx[0][1] = 0; Rx[0][2] = 0;
	Rx[1][0] = 0; Rx[1][1] = 0; Rx[1][2] = 0;
	Rx[2][0] = 0; Rx[2][1] = 0; Rx[2][2] = 0;
	return 0;
}
