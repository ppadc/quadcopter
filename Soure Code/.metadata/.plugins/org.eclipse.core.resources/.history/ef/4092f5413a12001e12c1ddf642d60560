 *      Rotational matrix that relates u,v,w with x_dot,y_dot,z_dot
 */
#include "rotational_matrix.h"
#include "params.h"
#include "user_define.h"
#include <math.h>
#include "bno055.h"

int get_rotational_matrix(float *roll, float *pitch,float *yaw, float *T_matrix[3][3]){
	uint8_t tmp;
	float Rx[3][3],Ry[3][3],Rz[3][3];
	float roll_in,pitch_in,yaw_in;
	roll_in  = *roll;
	pitch_in = *pitch;
	yaw_in = *yaw;
	bno055_get_elu_data(&roll_in, &pitch_in, &yaw_in);
	return 0;
}
