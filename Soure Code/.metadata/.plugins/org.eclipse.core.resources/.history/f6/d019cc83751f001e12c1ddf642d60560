float t[251]={0.0};
float t_angles[1001]={0.0};


int LPV_gereration_signals(){
	/*Generate the refence signals*/
	//Generate time refence
	for(int i=1;i<251;i++){
		t[i] = t[i] + 0.4;
	}
	//Generate time refence
	for(int i=1;i<1001;i++){
		t_angles[i] = t[i] + 0.4;
	}
	return 0;
}

